%tr{ :class => "#{cycle('odd', 'even')}"}
  %td#friendname
    = link_to friendship.friend.username,user_path(friendship.friend),:class => 'Relationship',:rel => rel_tag(@user, friendship),:id => "friend-#{friendship.friend.id}"
  %td#activity
    - if friendship.friend.last_activity_at
      = friendship.friend.last_activity
      = time_ago_in_words(friendship.friend.last_activity_at)
      ago