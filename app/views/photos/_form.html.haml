= error_messages_for :photo
= form_for(:photo,:url => user_photos_path(:user_id => @logged_in_user),:html => { :multipart => true }) do |f| %>
  %p Select a photo to upload
  %p Title:
    %br
    = f.text_field 'title'
  %p Description:
    %br
    = f.text_area 'body', :rows => 6, :cols => 40
  %p Photo:
    %br
    = f.file_field 'uploaded_data'
  %p
    = submit_tag 'Upload Photo'
    = link_to 'cancel', user_photos_path(current_user)